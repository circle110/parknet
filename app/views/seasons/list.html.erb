<div class="season list"><h2>Seasons</h2><%= link_to("Add New Season", :action => 'new', :class => 'action new') %><table summary="Seasons" class=listing">	<tr>		<th>Season</th>		<th>Season Year</th>		<th>Default Reg. Start Date</th>		<th>Default NR Reg. Start Date</th>		<th>Default Internet Display Date</th>		<th>Member Reg. Start Date</th>		<th>Reg. Start Time</th>		<th>Active</th>		<th>Actions</th>	</tr>	<% @seasons.each do |season| %>			<tr class="seasons">				<td><%= season.season_title.title %></td>				<td><%= season.season_year %></td>				<td><%= season.default_registration_start_date.strftime("%b. %d, %Y") %></td>				<td><%= season.default_nr_registration_start_date.strftime("%b. %d, %Y") %></td>				<td><%= season.default_internet_display_date.strftime("%b. %d, %Y") %></td>				<td><%= season.member_registration_start_date.strftime("%b. %d, %Y") %></td>				<td><%= season.registration_start_time.strftime("%I:%M %p") %></td>				<td><%= season.active ? " âœ“ ":"" %></td>				<td class="actions">				<%= link_to("Edit", {:action => 'edit', :id => season.id}, :class => 'action_edit') %>			</tr>	<% end %></table></div>	