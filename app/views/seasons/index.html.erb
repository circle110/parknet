<div class="season list"><h2>Seasons</h2><%= link_to("Add New Season", :action => 'new', :class => 'action new') %><% if @seasons.count > 0 %><table summary="Seasons" class=listing" cellpadding="4">	<tr>		<th>Season</th>		<th>Default Reg. Start Date</th>		<th>Default NR Reg. Start Date</th>		<th>Default Internet Display Date</th>		<th>Member Reg. Start Date</th>		<th>Reg. Start Time</th>		<th>Active</th>	</tr>	<% @seasons.each do |season| %>			<tr class="seasons">				<td><%= link_to("#{season.season_title.title} #{season.season_year}", {:action => 'edit', :id => season.id}, :class => 'action_edit') %></td>				<td><%= season.default_registration_start_date.strftime("%b. %d, %Y") %></td>				<td><%= season.default_nr_registration_start_date.strftime("%b. %d, %Y") %></td>				<td><%= season.default_internet_display_date.strftime("%b. %d, %Y") %></td>				<td><%= season.member_registration_start_date.strftime("%b. %d, %Y") %></td>				<td><%= season.registration_start_time.strftime("%I:%M %p") %></td>				<td><%= season.active ? " âœ“ ":"" %></td>	<% end %></table><% else %><p>No seasons have been defined in the database yet. Please click the "Add New Season" link above to add a season.<% end %></div>	