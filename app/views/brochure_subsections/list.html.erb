<div class="brochure subsection list"><h2>Brochure Subsections</h2><%= link_to("Add New Brochure Subsection", :action => 'new', :class => 'action new') %><% if @brochure_subsections.count > 0 %><table summary="Brochure Subsections" class=listing" cellpadding="5">	<tr>		<th>Name</th>		<th>Brochure Section </th>		<th>Active</th>			</tr>	<% @brochure_subsections.each do |brochure_subsection| %>			<tr class="brochure subsections">				<td><%= link_to("#{brochure_subsection.name}", {:action => 'edit', :id => brochure_subsection.id}, :class => 'action_edit') %></td>				<td><%= brochure_subsection.brochure_section.name %></td>				<td><%= brochure_subsection.active ? " âœ“ ":"" %></td>	<% end %></table><% else %>No brochure subsections have been defined in the database yet. Please click the "Add New Brochure Subsections" link above to add a brochure subsections.<% end %></div>	