<div class="show registrations"><% if /account/.match(request.env['HTTP_REFERER']) %>	<%= link_to("<<Back to Account", {:action => 'edit', :controller => 'accounts', :id => @customer.account_id}, :class => 'back-link') %><% else %>	<%= link_to("<<Back to Registration", {:action => 'registration', :controller => 'staff_registration', :id => @customer.account_id}, :class => 'back-link') %><% end %><h4>Withdraw <%= @customer.first_name %> <%= @customer.last_name %> from <%= @registration.class_session.program.name %></h4>	<ul>		<% if @registration.status_id == "l" %>			<% initial_verbage = "ON WAITLIST -- " %>			<% status_id = "d" %>		<% else %>			<% waitlist_verbage = "" %>			<% status_id = "w" %>		<% end %>				<li><%= initial_verbage %><%= @registration.class_session.program.name %> | <%= @registration.class_session.start_date.strftime("%a, %b %d, %Y") %> | 		<%= @registration.class_session.end_date.strftime("%a, %b %d, %Y") %> | 		<%= get_days_of_week(@registration.class_session_id) %> | 		<%= @registration.class_session.start_time.strftime("%I:%M%p") %> - <%= @registration.class_session.end_time.strftime("%I:%M%p") %> | 		</li> 	</ul>	<%= form_for @registration, :url => {:action => 'withdraw_registration', :controller => 'staff_registration'}, html: {class: "reg_form"} do |f| %>		<%= f.hidden_field(:registration_id, :value => @registration.id) %>		<%= f.hidden_field(:user_stamp, :value => session[:staff_user_id]) %>		<%= f.hidden_field(:customer_id, :value => @customer.id) %>		<%= select_tag "withdrawal_fee", options_for_select(AdditionalFee.where("agency_id = #{session[:agency_id]} AND fee_type = 'withdrawal' AND gl_account_id in (SELECT id from gl_accounts where fund_id = #{@registration.class_session.gl_account.fund_id})").collect{|u| ["#{u.name_amount}", u.id]}), :prompt => "--Apply Withdrawal Fee--" %><br>		<%= f.submit "Process Withdrawal" %>	<% end %></div>	