<h4>Class List <br><%= @class_list.first.class_session.program.name %> <%= @class_list.first.class_session.session_display_order %> <%= @class_list.first.class_session.season.season_title.title %> <%= @class_list.first.class_session.season.season_year %></h4>

<%= get_days_of_week(@class_list.first.class_session.id) %> <%= @class_list.first.class_session.start_time.strftime("%I:%M %p") %> - <%= @class_list.first.class_session.end_time.strftime("%I:%M %p")  %>
<br>
<%= @class_list.first.class_session.start_date.strftime("%a, %B %d")  %> - <%= @class_list.first.class_session.end_date.strftime("%a, %B %d") %>
<br>

<% @class_list.each do |c| %>

<%= c.customer.full_name %> <%= c.customer.birthdate.strftime("%B %d, %Y") %>(<%= c.customer.account.home_area %>)<%= c.customer.account.home_phone[0..2] %>-<%= c.customer.account.home_phone[3..6] %> <%= c.customer.email.nil? ? c.customer.account.email.to_s : c.customer.email.to_s %>

<br>
<% end %>