<div class="show registrations"><% if /account/.match(request.env['HTTP_REFERER']) %>	<%= link_to("<<Back to Account", {:action => 'edit', :controller => 'accounts', :id => @account.id}, :class => 'back-link') %><% else %>	<%= link_to("<<Back to Registration", {:action => 'registration', :controller => 'staff_registration', :id => @account.id}, :class => 'back-link') %><% end %><h4>Registrations for <%= @account.email %> || (<%= @account.home_area %>)<%= @account.home_phone[0..2] %>-<%= @account.home_phone[3..6] %></h4><% if @registrations.first.try(:id) %>	<ul>	<% @registrations.each do |reg| %>		<% if reg.status_id == "l" %>			<% initial_verbage = "ON WAITLIST -- " %>			<% status_id = "d" %>		<% else %>			<% waitlist_verbage = "" %>			<% status_id = "w" %>		<% end %>		<% if reg.status_id == "w" %>			<% initial_verbage = "WITHDRAWN -- " %>		<% end %>		<li><%= reg.customer.full_name %> | 		<%= initial_verbage %><%= reg.class_session.program.name %> | <%= reg.class_session.start_date.strftime("%a, %b %d, %Y") %> | 		<%= reg.class_session.end_date.strftime("%a, %b %d, %Y") %> | 		<%= get_days_of_week(reg.class_session_id) %> | 		<%= reg.class_session.start_time.strftime("%I:%M%p") %> - <%= reg.class_session.end_time.strftime("%I:%M%p") %> | 		<% if reg.status_id == "w" || reg.status_id == "d" %>			Already Withdrawn from class |		<% else %>			<%= link_to("Withdraw", {:action => 'withdraw', :controller => 'registrations', :registration_id => reg.id, :customer_id => reg.customer.id, 		:class_session_id => reg.class_session_id, :status_id => status_id}, :class => 'back-link') %> |		<% end %>		</li> 	<% end %>	</ul><% else %>This account has no current or past registrations.<% end %></div>	