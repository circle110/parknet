<div class="fund list"><h2>Funds</h2><%= link_to("Add New Fund", {:action => 'new'}, :class => 'add_new_item') %><p><table summary="Fund" class=listing">	<tr>		<th>Fund</th>		<th>Name</th>		<th>Active</th>		<th>Actions</th>	</tr>	<% @funds.each do |fund| %>			<tr class="fund_list">				<td><%= fund.fund %></td>				<td><%= fund.name %></td>				<td><%= fund.active %></td>				<td class="actions">				<%= link_to("Edit", {:action => 'edit', :id => fund.id}, :class => 'action_edit') %>			</tr>		<%# end %>	<% end %></table></div>	