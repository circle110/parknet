<div class="gl_account list"><h2>GL Accounts</h2><%= link_to("Add New GL Account", :action => 'new', :class => 'action new') %><% if @gl_accounts.count > 0 %><table summary="Gl Accounts" class=listing" cellpadding="4">	<tr>		<th>Name</th>		<th>Fund</th>		<th>Gl Account Number</th>		<th>Account Type</th>		<th>Active</th>	</tr>	<% @gl_accounts.each do |gl_account| %>			<tr class="staff_users">				<td><%= link_to("#{gl_account.name}", {:action => 'edit', :id => gl_account.id}, :class => 'action_edit') %></td>				<td><%= gl_account.fund.fund %> - <%= gl_account.fund.name %></td>				<td><%= gl_account.gl_account_number %></td>				<td>				<% gl_type = gl_account.account_type %>				<% case  gl_type								   when 1 %>						Bank Charges					<% when 2 %>						Program Revenue					<% when 3 %>						Customer Account Balances					<% when 4 %>						Deferred Revenue					<% when 5 %>						Extra Fee Revenue					<% when 6 %>						Cash					<% when 7 %>						Credit Card					<% when 8 %>						Refund Pending					<% when 9 %>						Tax				 <% end %>				</td>				<td><%= gl_account.active ? "âœ“":"" %></td>	<% end %></table><% else %><p>No GL accounts have been defined in the database yet. Please click the "Add New GL Account" link above to add a GL Account.<% end %></div>	