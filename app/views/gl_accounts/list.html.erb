<div class="gl_account list"><h2>GL Accounts</h2><%= link_to("Add New Gl Account", :action => 'new', :class => 'action new') %><table summary="Gl Accounts" class=listing" cellpadding="4">	<tr>		<th>Name</th>		<th>Fund</th>		<th>Gl Account Number</th>		<th>Account Type</th>		<th>Active</th>	</tr>	<% @gl_accounts.each do |gl_account| %>			<tr class="staff_users">				<td><%= link_to("#{gl_account.name}", {:action => 'edit', :id => gl_account.id}, :class => 'action_edit') %></td>				<td><%= gl_account.fund.fund %> - <%= gl_account.fund.name %></td>				<td><%= gl_account.gl_account_number %></td>				<td><%= gl_account.account_type %></td>				<td><%= gl_account.active ? "âœ“":"" %></td>	<% end %></table></div>	