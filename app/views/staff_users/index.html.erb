<div class="staff_user list"><h2>Staff Users</h2><%= link_to("Add New User", :action => 'new', :class => 'action new') %><% if @staff_users.count > 0 %><table summary="Staff User" class=listing" cellpadding="4">	<tr>		<th>Name</th>		<th>Email - Login Id</th>	</tr>	<% @staff_users.each do |user| %>			<tr class="staff_users">				<td><%= link_to("#{user.last_name}, #{user.first_name}", {:action => 'edit', :id => user.id}, :class => 'action_edit') %></td>				<td><%= user.email %></td>	<% end %></table><% else %>No users have been defined in the database yet. Please click the "Add New User" link above to add a user.<% end %></div>	