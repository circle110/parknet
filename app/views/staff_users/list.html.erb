<div class="staff_user list"><h2>Staff Users</h2><%= link_to("Add New User", :action => 'new', :class => 'action new') %><% if @staff_users.count > 0 %><table summary="Staff User" class=listing">	<tr>		<th>Name</th>		<th>Email - Login Id</th>		<th>Security Level</th>		<th>Actions</th>	</tr>	<% @staff_users.each do |user| %>			<tr class="staff_users">				<td><%= user.first_name %> <%= user.last_name %></td>				<td><%= user.email %></td>				<td><%= user.security_group %></td>				<td class="actions">				<%= link_to("Edit", {:action => 'edit', :id => user.id}, :class => 'action_edit') %>			</tr>	<% end %></table><% else %>No users have been defined in the database yet. Please click the "Add New User" link above to add a user.<% end %></div>	