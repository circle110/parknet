<div class="program list"><h2>Programs</h2><%= link_to("Add New Program", :action => 'new', :class => 'action new') %><% if @programs.count > 0 %><table summary="Programs" class=listing">	<tr>		<th>Name</th>		<th>Code</th>		<th>Actions</th>			</tr>	<% @programs.each do |program| %>			<tr class="programs">				<td><%= program.name %></td>				<td><%= program.code %></td>				<td class="actions">				<%= link_to("Edit", {:action => 'edit', :id => program.id}, :class => 'action_edit') %>				</td>			</tr>	<% end %></table><% else %>No programs have been defined in the database yet. Please click the "Add New Program" link above to add a program.<% end %></div>	