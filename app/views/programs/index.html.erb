<div class="program list"><h2>Programs</h2><%= link_to("Add New Program", :action => 'new', :class => 'action new') %><% if @programs.count > 0 %><table summary="Programs" class=listing" cellpadding="4">	<tr>		<th><%= link_to("Name", :action => 'index', :class => 'action resort', :sort => 'name') %></th>		<th><%= link_to("Code", :action => 'index', :class => 'action resort', :sort => 'code') %></th>			</tr>	<% @programs.each do |program| %>			<tr class="programs">				<td><%= link_to("#{program.name}", {:action => 'edit', :id => program.id}, :class => 'action_edit') %></td>				<td><%= program.code %></td>			</tr>	<% end %></table><% else %><p>No programs have been defined in the database yet. Please click the "Add New Program" link above to add a program.<% end %></div>	