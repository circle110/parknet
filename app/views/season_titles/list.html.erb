<div class="season title list"><h2>Season Titles</h2><%= link_to("Add New Season Title", :action => 'new', :class => 'action new') %><% if @season_titles.count > 0 %><table summary="Season Titles" class=listing">	<tr>		<th>Season Title</th>		<th>Active</th>		<th>Actions</th>	</tr>	<% @season_titles.each do |season_title| %>			<tr class="staff_users">				<td><%= season_title.title %></td>				<td><%= season_title.active ? " âœ“ ":"" %></td>				<td class="actions">				<%= link_to("Edit", {:action => 'edit', :id => season_title.id}, :class => 'action_edit') %>			</tr>	<% end %></table><% else %>No season titles have been defined in the database yet. Please click the "Add New Season Title" link above to add a season title.<% end %></div>	