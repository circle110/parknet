<div class="season title list"><h3>Season Titles</h3><%= link_to("Add New Season Title", :action => 'new', :class => 'action new') %><% if @season_titles.count > 0 %><table summary="Season Titles" class=listing" cellpadding="4">	<tr>		<th>Season Title</th>		<th>Active</th>	</tr>	<% @season_titles.each do |season_title| %>			<tr class="staff_users">				<td><%= link_to("#{season_title.title}", {:action => 'edit', :id => season_title.id}, :class => 'action_edit') %></td>				<td><%= season_title.active ? " âœ“ ":"" %></td>	<% end %></table><% else %><p>No season titles have been defined in the database yet. Please click the "Add New Season Title" link above to add a season title.<% end %></div>	