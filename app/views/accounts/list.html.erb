<div class="account list"><h2>Accounts Found</h2><%= link_to("Add New Account", :action => 'new', :class => 'action new') %><table summary="Account" class=listing">	<tr>		<th>Name</th>		<th>Phone</th>		<th>Email - Login Id</th>		<th>Address</th>		<th>Actions</th>	</tr>	<% @customers.each do |cust| %>			<tr class="staff_users">				<td><%= cust.first_name %> <%= cust.last_name %></td>				<td>(<%= cust.account.home_area %>) <%= cust.account.home_phone[0..2] %>-<%= cust.account.home_phone[3..6] %></td>				<td><%= cust.account.email %></td>				<td><%= cust.account.address %> <%= cust.account.address_2 %> <%= cust.account.city.name %>, <%= cust.account.state %> <%= cust.account.zip %></td>				<td class="actions">				<%= link_to("Select", {:action => 'edit', :id => cust.account.id}, :class => 'action_edit') %>				<%#= link_to("Select", {:action => 'edit', :id => @accounts.first['id']}, :class => 'action_edit') %>			</tr>	<% end %></table></div>	