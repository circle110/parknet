<div class="account list"><h2>Maintain Accounts - Search</h2><%= form_tag '/accounts/list', html: {class: "input_form"} do %>	<%= hidden_field_tag(:agency_id, :value => session[:agency_id]) %>	<%= hidden_field_tag(:user_stamp, :value => session[:staff_user_id]) %>	<table summary="Account" class=listing">	<tr>		<th>Last Name</th>		<th>First Name</th>		<th>Phone Number</th>	</tr>	<tr>		<td><%= text_field_tag(:last_name) %></td>		<td><%= text_field_tag(:first_name) %></td>		<td><%= text_field_tag(:home_phone) %></td>	</tr>	<tr>		<td><%= submit_tag "Search for account" %></td>	</tr></table><% end %></div>	