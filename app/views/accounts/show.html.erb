<div class="account search"><h2>Maintain Accounts - Search</h2><%= form_tag '/accounts/list', html: {class: "input_form"} do %>	<%= hidden_field_tag(:agency_id, :value => session[:agency_id]) %>		<%= label_tag 'search_text', 'Search For' %>	<%= text_field_tag(:search_text) %>	<%= submit_tag "Search for account" %><% end %></div>	