<div class="brochure section list"><h3>Brochure Sections</h3><%= link_to("Add New Brochure Section", :action => 'new', :class => 'action new') %><table summary="Brochure Sections" class=listing">	<tr>		<th>Brochure Section Id</th>		<th>Agency Id</th>		<th>Name</th>		<th>Active</th>		<th>Description</th>			</tr>	<% @brochure_sections.each do |brochure_section| %>			<tr class="brochure sections">				<td><%= brochure_section.id %></td>				<td><%= brochure_section.agency_id %></td>				<td><%= brochure_section.name %></td>				<td><%= brochure_section.active %></td>				<td><%= brochure_section.description %></td>				<td class="actions">				<%= link_to("Show", {:action => 'show', :id => brochure_section.id}, :class => 'action_show') %>				<%= link_to("Edit", {:action => 'edit', :id => brochure_section.id}, :class => 'action_edit') %>				<%= link_to("Delete", '#', :class => 'action_delete') %>			</tr>	<% end %></table></div>########## below is the code for nested <li> presentation<div class="brochure section list"><h2>Maintain Programs</h2><%= link_to("Add New Brochure Section", :action => 'new', :class => 'action new') %>	<ul>	<% @brochure_sections.each do |brochure_section| %>					<li><%= link_to(brochure_section.name, {:action => 'edit', :id => brochure_section.id}, :class => 'action_edit') %></li>				<% subsections = BrochureSubsection.where("brochure_section_id = ?", brochure_section.id) %>				<% if subsections %>					<ul>					<% subsections.each do |subsection| %>						<li><%= link_to(subsection.name, {:action => 'edit', :controller => 'brochure_subsections', :id => subsection.id}, :class => 'action_edit') %></li>						<% programs = Program.where("brochure_subsection_id = ?", subsection.id) %>						<% if programs %>							<ul>							<% programs.each do |program| %>								<li><%= link_to(program.name, {:action => 'edit', :controller => 'programs', :id => program.id}, :class => 'action_edit') %></li>							<% end %>							</ul>						<% end %>					<% end %>					</ul>				<% end %>	<% end %>	</ul></div>		